# Advent of Code (AoC) - day 10, task 1

import "prelude.eg"

using System, OS, List, String (from_chars, to_chars), R = Regex, D = Dict

def main =
    read_lines stdin |> map to_int |> sort |> [XX -> XX ++ {last XX + 3}] 
    |> [XX -> zip XX {0|XX}] |> map (uncurry (-))
    |> [XX -> (filter ((==) 1) XX |> length) * (filter ((==) 3) XX |> length)]
